{{> navbar}}
<div class="container">
	<h4>Last scanning action</h4>
	<h4>{{scanned}}</h4>
	<script src="/socket.io/socket.io.js"></script>

	<ul id="scans"></ul>

	<script> 
	 	var socket = io();
	    socket.on('scans', function (msg) {
	       $('#scans').append($('<li>').text(msg));
	    });
	</script>
</div>
	   